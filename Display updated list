#include <stdio.h>

int main() {
    int n, newRecords, i;

    printf("Enter number of existing students: ");
    scanf("%d", &n);

    int marks[100]; // max 100 students

    printf("Enter marks for %d students:\n", n);
    for(i = 0; i < n; i++)
        scanf("%d", &marks[i]);

    printf("Enter number of new students to add: ");
    scanf("%d", &newRecords);

    for(i = n; i < n + newRecords; i++) {
        printf("Enter marks for student %d: ", i + 1);
        scanf("%d", &marks[i]);
    }

    int total = n + newRecords;

    printf("\nUpdated Marks List:\n");
    for(i = 0; i < total; i++)
        printf("Student %d: %d\n", i + 1, marks[i]);

    return 0;
}
