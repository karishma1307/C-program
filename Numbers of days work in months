#include <stdio.h>

int main() {
    int days[12];
    int i, totalDays = 0;

    printf("Enter number of days worked for each month:\n");

    for (i = 0; i < 12; i++) {
        printf("Month %d: ", i + 1);
        scanf("%d", &days[i]);
        totalDays += days[i];
    }

    printf("Total number of days worked in the year = %d\n", totalDays);

    return 0;
}
