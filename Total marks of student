#include <stdio.h>

int main() {
    int marks1, marks2, marks3, marks4, marks5;
    int total;

    printf("Enter marks of 5 subjects:\n");
    scanf("%d %d %d %d %d", &marks1, &marks2, &marks3, &marks4, &marks5);

    total = marks1 + marks2 + marks3 + marks4 + marks5;

    printf("Total Marks = %d\n", total);

    return 0;
}
