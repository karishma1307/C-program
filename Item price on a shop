#include <stdio.h>

int main() {
    int n, i;
    float price[50], searchPrice;
    int found = 0;

    printf("Enter number of items: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter price of item %d: ", i + 1);
        scanf("%f", &price[i]);
    }

    printf("Enter price to search: ");
    scanf("%f", &searchPrice);

    for (i = 0; i < n; i++) {
        if (price[i] == searchPrice) {
            printf("Item found at position %d\n", i + 1);
            found = 1;
            break;
        }
    }

    if (found == 0) {
        printf("Item with given price not found\n");
    }

    return 0;
}
