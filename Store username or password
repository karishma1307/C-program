#include <stdio.h>
#include <string.h>

struct Login
{
    char username[20];
    char password[20];
};

int main()
{
    struct Login user;

    // Store credentials
    strcpy(user.username, "admin");
    strcpy(user.password, "1234");

    char uname[20], pwd[20];

    // Input from user
    printf("Enter Username: ");
    scanf("%s", uname);

    printf("Enter Password: ");
    scanf("%s", pwd);

    // Verify login
    if(strcmp(uname, user.username) == 0 && strcmp(pwd, user.password) == 0)
        printf("Login Successful\n");
    else
        printf("Invalid Username or Password\n");

    return 0;
}
